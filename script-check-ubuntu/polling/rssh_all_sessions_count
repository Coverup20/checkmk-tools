#!/bin/bash
# Launcher base per eseguire uno script remoto dal repo GitHub
# Inserisci l'URL dello script qui sotto

SCRIPT_URL="https://raw.githubusercontent.com/Coverup20/checkmk-tools/main/script-check-ubuntu/polling/ssh_all_sessions_count"

# Puoi passare parametri aggiuntivi allo script remoto
# Uso: bash rssh_all_sessions_count [parametri]

bash <(curl -fsSL "$SCRIPT_URL") "$@"
